ndition.sol\":{\"keccak256\":\"0x0894bd9d23e57b65b6945ade14bc3152253a70d5bd206e9c02679df1ccbacc59\",\"license\":\"Apache-2.0\",\"urls\":[\"bzz-raw://f7d5ed75c623cfaf89fd3961cc2e204f423e5406ad8f7d0c55d6f7c2767146c0\",\"dweb:/ipfs/QmTxcqLiyQiik23bFojYmmDXWVyj1XLT3UY8nbpwygJF16\"]},\"@thirdweb-dev/contracts/extension/interface/IClaimConditionMultiPhase.sol\":{\"keccak256\":\"0xc9f8f3dd23deecc443aabe696227394d0d8089a4ee1a7d381f8ee4513724ed82\",\"license\":\"Apache-2.0\",\"urls\":[\"bzz-raw://b8bf4f0e85f246c97328d1764816e8b420ff96ae7ea693e8d43d4692a88dff1d\",\"dweb:/ipfs/QmNYZQkYx3kcjsjub4gEhQwGDfboYYSoK7VuVSmoFS4wWn\"]},\"@thirdweb-dev/contracts/extension/interface/IDrop.sol\":{\"keccak256\":\"0xeebc15dcb0e826e5c665d0fc35fcbf37b55d3ec15462c6e03caa003f91438d19\",\"license\":\"Apache-2.0\",\"urls\":[\"bzz-raw://fed879b9ebc99208219b30e7587652959c3d877b53f29e2b0a501413ef3ac834\",\"dweb:/ipfs/QmNmdmzHMbwWL3AU2dpx1FJJs7fEsrP3TJiWQY1jwAerZA\"]}},\"version\":1}",
