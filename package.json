  "build": {
    "files": [
      {
        "from": "node_modules/ganache/node_modules/leveldown/prebuilds/${platform}-${arch}",
        "to": "dist/${platform}-${arch}"
      }
    ]
  },
  "scripts": {
    "testeranto-clean": "rm -rf docs && mkdir docs",
    "testeranto-esbuild": "ts-node-esm testeranto.mts",
    "testeranto-esbuild-dev": "ts-node-esm testeranto.mts -dev",
    "testeranto-puppeteer": "ts-node-esm node_modules/testeranto/dist/module/Puppeteer.js",
    "testeranto-reporter": "cp ./node_modules/testeranto/dist/prebuild/Report.js docs/Report.js && node ./node_modules/testeranto/dist/module/Reporter.js"
  },
  "types": "index.d.ts",
  "dependencies": {
    "@types/node": "^20.12.2",
    "esbuild": "0.17.11",
    "fluent-ffmpeg": "^2.1.2",
    "html": "^1.0.0",
