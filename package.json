{
  "name": "kokomo-bay",
  "description": "",
  "type": "module",
  "version": "0.2.0",
  "repository": "git@github.com:ChromaPDX/kokomoBay.git",
  "homepage": "https://github.com/ChromaPDX/kokomoBay",
  "author": "adam wong <adamwong246@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "18.18.0"
  },
  "keywords": [
    "typescript",
    "test",
    "cucumber",
    "ATDD"
  ],
  "build": {
    "files": [
      {
        "from": "node_modules/ganache/node_modules/leveldown/prebuilds/${platform}-${arch}",
        "to": "dist/${platform}-${arch}"
      }
    ]
  },
  "scripts": {
    "clean": "rm -rf docs && mkdir docs",
    "build-tests": "rm -rf docs/node/chunk-*; rm -rf docs/web/chunk-*; rm -rf docs/types/ ; tsx node_modules/testeranto/dist/prebuild/build.js testeranto.mts",
    "run-tests": "tsx node_modules/testeranto/dist/prebuild/run.js testeranto.mts",
    "init-docs": "tsx node_modules/testeranto/dist/prebuild/init-docs.mjs testeranto.mts",
    "report": "tsx node_modules/testeranto/dist/prebuild/ReportServer.mjs testeranto.mts",
    "test": "yarn build-tests && yarn run-tests",
    "aider": "aider --model deepseek/deepseek-chat"
  },
  "types": "index.d.ts",
  "dependencies": {
    "@ethersproject/shims": "^5.7.0",
    "@types/express": "^5.0.0",
    "@types/node": "^20.12.2",
    "esbuild": "0.17.11",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "fluent-ffmpeg": "^2.1.2",
    "html": "^1.0.0",
    "http-shutdown": "^1.2.2",
    "node-fetch": "^3.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.4",
    "react-use-error-boundary": "^3.0.0",
    "ts-node": "^10.9.2",
    "tsc-module-loader": "^0.0.1",
    "tsx": "^4.19.1",
    "typescript-eslint": "^8.29.0",
    "url": "^0.11.3"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@openzeppelin/contracts": "^4.8.0",
    "@reduxjs/toolkit": "^1.8.6",
    "@testing-library/react": "^13.4.0",
    "@thirdweb-dev/contracts": "^3.3.0",
    "@thirdweb-dev/sdk": "^3.6.9",
    "@truffle/compile-solidity": "6.0.56",
    "@truffle/contract": "^4.6.10",
    "@types/chai": "^4.3.4",
    "@types/lodash": "^4.14.189",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.23",
    "@types/react-test-renderer": "18.0.7",
    "@types/web3": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "babel-loader": "^8.2.5",
    "chai": "^4.3.7",
    "eslint": "9.23.0",
    "eslint-plugin-react": "^7.37.4",
    "ethers": "^5.7.2",
    "ganache": "^7.9.2",
    "ignore-loader": "^0.1.2",
    "nodemon": "^2.0.20",
    "react-test-renderer": "18.2.0",
    "testeranto": "0.85.0",
    "truffle": "^5.7.3",
    "ts-loader": "^9.3.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8.2",
    "web3": "^1.8.1",
    "web3-eth-contract": "^1.8.1"
  }
}